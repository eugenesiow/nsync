<Page x:Class="nsync.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="SettingsPage" Width="518" Height="287">
    <Grid>
        <Label />
        <Image x:Name="ImageSettings" Margin="5" VerticalAlignment="Top" Height="45" Source="Resources/settings.png" Width="151"/>
        <!--<Label VerticalAlignment="Top" Name="LabelSettings" FontSize="30" Foreground="White" Height="55" Margin="201,1,201,0">Settings</Label>-->
        <StackPanel Margin="20,55,20,20" Name="PanelSettings" Style="{StaticResource PanelSettingsStyle}">
            <CheckBox Height="16" Style="{StaticResource CheckBoxSettingsStyle}" HorizontalAlignment="Left" Margin="20, 20" Name="CheckboxToggleHelperWindow" VerticalAlignment="Top" Width="165" Foreground="White" Checked="CheckboxToggleHelperWindow_Checked" Unchecked="CheckboxToggleHelperWindow_UnChecked">Disable Helper Window</CheckBox>
        </StackPanel>
    </Grid>
</Page>
