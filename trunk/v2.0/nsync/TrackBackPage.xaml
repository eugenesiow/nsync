<Page x:Class="nsync.TrackBackPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="TrackBackPage" Width="518" Height="287" Loaded="Page_Loaded">
    <Grid>
        <Label />
        <Image x:Name="ImageTrackBack" Margin="5" VerticalAlignment="Top" Height="45" Source="Resources/trackback.png" Width="210"/>
        <ListView Margin="20, 100, 20, 30" Foreground="White" MaxHeight="410" MaxWidth="500" ItemsSource="{Binding trackBackCollection}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="100" Header="Name" DisplayMemberBinding="{Binding nameItem}"  />
                    <GridViewColumn Width="100" Header="Date"  
           DisplayMemberBinding="{Binding dateItem}" />
                    <GridViewColumn Width="100" Header="To Folder" 
           DisplayMemberBinding="{Binding folderItem}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
